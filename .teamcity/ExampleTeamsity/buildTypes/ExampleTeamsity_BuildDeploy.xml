<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="705c3af9-93d4-4fa1-95fd-87493522f0ed" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build&amp;Deploy</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="Make Some Test" type="Maven2">
        <conditions>
          <does-not-contain name="teamcity.build.branch" value="master" />
        </conditions>
        <parameters>
          <param name="goals" value="clean test" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="runnerArgs" value="-Dmaven.test.failure.ignore=true" />
          <param name="teamcity.step.mode" value="default" />
          <param name="userSettingsSelection" value="custom" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Deploy to Nexus" type="Maven2">
        <conditions>
          <contains name="teamcity.build.branch" value="master" />
        </conditions>
        <parameters>
          <param name="goals" value="clean package deploy" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="runnerArgs" value="-Dmaven.test.failure.ignore=true" />
          <param name="teamcity.step.mode" value="default" />
          <param name="userSettingsSelection" value="custom" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ExampleTeamsity_HttpsGithubComPopovNA91exampleTeamcityGitRefsHeadsMaster1" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

